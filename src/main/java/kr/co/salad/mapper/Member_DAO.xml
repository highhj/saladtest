<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
    
    
<!-- namespace="dao.NoticeDao 인터페이스 동일 경로 
     구현된되는 ID 값들을 인터페이스 함수명과 동일
 -->
<mapper namespace="kr.co.salad.dao.Memer_DAO">
 <insert id="insert">
      <selectKey order="BEFORE" keyProperty="seq" resultType="String">
       SELECT NVL(MAX(TO_NUMBER(SEQ)),0)+1 FROM member
      </selectKey>
     INSERT INTO MEMBER VALUES(seq_m_num.nextval, 'admin@kosta.com', '10041004', 'admin', '관리', '관리', '010-1234-1234', 1)
</insert>
   
   
</mapper>

